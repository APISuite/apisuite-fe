{
  "name": "appcenter-frontend",
  "version": "1.3.0",
  "description": "appcenter-frontend",
  "main": "dist/index.js",
  "scripts": {
    "local": "source local.conf && cross-env NODE_ENV=development THEME=default webpack-dev-server",
    "local:bnpp": "source local.conf && cross-env NODE_ENV=development THEME=bnpp webpack-dev-server",
    "dev": "cross-env NODE_ENV=development THEME=default webpack-dev-server",
    "dev:bnpp": "cross-env NODE_ENV=development THEME=bnpp webpack-dev-server",
    "mock-api": "node __mocks__/index.js",
    "prebuild": "yarn test && rimraf dist",
    "build": "cross-env NODE_ENV=production THEME=default webpack --progress",
    "build:bnpp": "cross-env NODE_ENV=production THEME=bnpp webpack --progress",
    "test": "cross-env NODE_ENV=test jest --c ./jest.json --coverage",
    "commit": "git-cz",
    "prestart": "yarn build",
    "start": "serve -s dist -l 8080",
    "open-cov": "open coverage/lcov-report/index.html",
    "generate": "node ./internals/generate",
    "release": "standard-version"
  },
  "contributors": [
    "muffles <molefas@gmail.com>",
    "cbalves <claudio@cloudoki.com>"
  ],
