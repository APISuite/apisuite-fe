{
  "name": "openbank-sandbox-auth-server",
  "version": "0.6.0",
  "description": "Openbank sandboxes auth server",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=knex:* NODE_PATH=. node src/index.js",
    "dev": "npm-run-all --parallel start build",
    "lint": "eslint src/ --ext .js",
    "debug": "NODE_PATH=. node src/index.js | pino-pretty",
    "test": "OBSANDBOXAUTH_API_LOG_ENABLED=false lab -I '__core-js_shared__' -r console -o stdout -r html -o test/coverage.html --context-timeout 2000 --timeout 4000 -t 50",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "release": "standard-version",
    "commit": "git-cz",
    "build": "webpack -d",
    "build-prod": "webpack -p",
    "server": "webpack-dev-server --open",
    "build-local": "source local.conf && webpack -d",
    "local": "source local.conf && NODE_PATH=. node src/index.js | pino-pretty",
    "audit": "yarn audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cloudoki/openbank-sandbox-auth-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cloudoki/openbank-sandbox-auth-server/issues"
  },
  "pre-commit": [
    "lint",
    "audit",
    "test"
  ],
  "homepage": "https://github.com/Cloudoki/openbank-sandbox-auth-server#readme",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.1",
    "bcrypt": "^3.0.2",
    "bookshelf": "^0.13.3",
    "bookshelf-modelbase": "^2.10.4",
    "convict": "^4.4.0",
    "core-js": "3.0.1",
    "css-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^17.6.0",
    "hapi-auth-bearer-token": "^6.1.1",
    "hapi-auth-cookie": "^9.0.0",
    "hapi-i18n": "^2.0.0",
    "hapi-x-request-id": "^1.1.1",
    "inert": "^5.1.0",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "pino": "^5.8.0",
    "pino-pretty": "^2.2.3",
    "promise-polyfill": "^8.1.0",
    "pug": "^2.0.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "vision": "^5.4.0",
    "whatwg-fetch": "^3.0.0",
    "yar": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chance": "^1.0.16",
    "code": "^5.2.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "git-cz": "^1.7.1",
    "lab": "^17.0.2",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "sinon": "^7.1.0",
    "standard-version": "^4.4.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11"
  },
  "engines": {
    "node": ">=8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "\nPlease use yarn commit or npm run commit to help you out."
    }
  },
  "standard-version": {
    "message": "chore(release): %s \n\n[ci skip]"
  },
  "resolutions": {
    "commitizen/find-node-modules/findup-sync/micromatch/braces": "2.3.1",
    "lab/handlebars": "4.1.2",
    "eslint-utils": "1.4.1",
    "standard-version/conventional-changelog/conventional-changelog-core/conventional-changelog-writer/handlebars": "4.1.2",
    "sinon/nise/just-extend": "4.0.2",
    "lodash": "4.17.12",
    "set-value": "3.0.1",
    "mixin-deep": "2.0.1"
  }
}
