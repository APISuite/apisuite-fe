    "semver": "6.2.0",
    "short-unique-id": "1.1.1",
    "speakeasy": "2.0.0",
    "standard-version": "5.0.2",
    "swagger2openapi": "5.3.0",
    "twilio": "3.19.2",
    "uuid": "3.2.1",
    "vision": "5.3.2",
    "wreck": "14.1.3"
  },
  "devDependencies": {
    "chance": "1.0.16",
    "code": "5.2.0",
    "eslint": "4.19.1",
    "jsdoc": "3.5.5",
    "lab": "18.0.2",
    "moment": "2.24.0",
    "pre-commit": "1.2.2",
    "sinon": "5.0.10"
  },
  "resolutions": {
    "eslint-utils": "1.4.1",
    "hapi-swagger/handlebars": "4.1.2",
    "lab/handlebars": "4.1.2",
    "sinon/just-extend": "4.0.0",
