{
  "name": "apisuite-middleware-api",
  "version": "1.8.4",
  "description": "APISuite Middleware API",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_PATH=. node src/index.js",
    "lint": "eslint src/ --ext .js",
    "debug": "DEBUG=knex:* NODE_PATH=. node src/index.js | bunyan",
    "test": "APPCENTER_API_LOG_LEVEL=fatal APISUITE_MIDDLEWARE_API_RUNNING_TESTS=true lab -I '__core-js_shared__,core' -r console -o stdout -r html -o test/coverage.html --context-timeout 2000 --timeout 4000 -t 75",
    "doc": "jsdoc -d docs -r src/",
    "migrate": "knex migrate:latest",
    "audit": "yarn audit",
    "rollback": "knex migrate:rollback",
    "release": "standard-version",
    "local": "source local.conf && DEBUG=knex:* NODE_PATH=. node src/index.js | bunyan"
  },
  "pre-commit": [
    "lint",
    "audit",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cloudoki/openbank-middleware-api.git"
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/Cloudoki/openbank-middleware-api/issues"
  },
  "homepage": "https://github.com/Cloudoki/openbank-middleware-api#readme",
  "dependencies": {
    "@octokit/app": "2.2.4",
    "@octokit/rest": "16.25.6",
    "@sendgrid/mail": "6.2.1",
    "ajv": "6.10.2",
    "bcrypt": "3.0.4",
    "bell": "9.3.1",
    "bluebird": "3.5.5",
    "bookshelf": "0.13.3",
    "bookshelf-modelbase": "2.10.4",
    "boom": "7.2.0",
    "bunyan": "1.8.12",
    "catbox-memory": "4.0.1",
    "convict": "4.2.0",
    "good": "8.1.1",
    "good-bunyan": "2.0.0",
    "handlebars": "4.1.2",
    "hapi": "17.3.1",
    "hapi-auth-bearer-token": "6.0.1",
    "hapi-swagger": "9.4.2",
    "inert": "5.1.0",
    "joi": "13.2.0",
    "jsonwebtoken": "8.2.1",
    "knex": "0.14.6",
    "marked": "0.7.0",
    "mock-knex": "0.4.6",
